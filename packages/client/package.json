{
	"name": "client",
	"version": "0.1.0",
	"description": "LightWeight Framework for Discord API Supported Javascript / Typescript Music Bots and Radio Bots with fast moderation with commands and no memory leak mode",
	"main": "dist",
	"types": "typings",
	"module": "./src/index.mjs",
	"files": [
		"src",
		"typings"
	],
	"exports": {
		"./*": "./*",
		".": {
			"require": "./src/index.js",
			"import": "./src/index.mjs"
		}
	},
	"scripts": {
		"build": "pnpm clear && tsc",
		"clear": "rimraf dist typings"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"engines": {
		"node": ">= 16.6.0",
		"npm": ">=8.0.0"
	},
	"dependencies": {
		"eris": "^0.17.1"
	},
	"directories": {
		"example": "example"
	},
	"author": "Sid is Live YT",
	"repository": {
		"url": "Guild-Of-Jericho/Jericho-Player",
		"directory": "packages/client"
	},
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/Guild-Of-Jericho/Jericho-Player/issues"
	},
	"homepage": "https://jericho-player.js.org",
	"keywords": [],
	"devDependencies": {
		"@types/node": "^18.11.9",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3"
	}
}
