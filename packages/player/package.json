{
	"name": "player",
	"version": "0.1.0",
	"description": "LightWeight Framework for Discord API Supported Javascript / Typescript Music Bots and Radio Bots with fast moderation with commands and no memory leak mode",
	"main": "dist",
	"types": "typings",
	"module": "./src/index.mjs",
	"files": [
		"src",
		"typings"
	],
	"exports": {
		"./*": "./*",
		".": {
			"require": "./src/index.js",
			"import": "./src/index.mjs"
		}
	},
	"scripts": {
		"build": "pnpm clear && tsc",
		"clear": "rimraf dist typings"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"engines": {
		"node": ">= 16.6.0",
		"npm": ">=8.0.0"
	},
	"dependencies": {
		"eris": "^0.17.1",
		"events": "^3.3.0",
		"ffmpeg-static": "^5.1.0",
		"fluent-ffmpeg": "^2.1.2",
		"isomorphic-unfetch": "^3.1.0",
		"linkifyjs": "^4.0.2",
		"node-youtube-music": "^0.8.3",
		"play-dl": "^1.9.6",
		"scdl-core": "^1.1.0",
		"spotify-url-info": "^3.1.9",
		"spotify-web-api-node": "^5.0.2"
	},
	"directories": {
		"example": "example"
	},
	"author": "Sid is Live YT",
	"repository": {
		"url": "Jericho/jericho-player",
		"directory": "packages/player"
	},
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/SidisLiveYT/Jericho-Player/issues"
	},
	"homepage": "https://jericho-player.js.org",
	"keywords": [],
	"devDependencies": {
		"@types/events": "^3.0.0",
		"@types/fluent-ffmpeg": "^2.1.20",
		"@types/node": "^18.11.9",
		"@types/spotify-web-api-node": "^5.0.7",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3"
	}
}
